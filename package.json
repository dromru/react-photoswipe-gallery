{
  "name": "react-photoswipe-gallery",
  "version": "1.2.0",
  "description": "React component wrapper around PhotoSwipe",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "photoswipe",
    "react",
    "gallery"
  ],
  "repository": "https://github.com/dromru/react-photoswipe-gallery.git",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.json",
    "start": "yarn build --watch",
    "test": "jest",
    "prepublish": "yarn build",
    "lint": "eslint",
    "commit": "git-cz",
    "prettify": "prettier",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "changelog": "conventional-changelog -i CHANGELOG.md -s",
    "release": "yarn version && yarn changelog"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@storybook/addon-knobs": "6.0.0-beta.0",
    "@storybook/addon-storysource": "6.0.0-beta.0",
    "@storybook/addons": "6.0.0-beta.0",
    "@storybook/react": "6.0.0-beta.0",
    "@storybook/source-loader": "6.0.0-beta.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.1",
    "@types/photoswipe": "^4.1.0",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.31",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.6",
    "photoswipe": "^4.1.3",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "photoswipe": ">= 4.1.0",
    "prop-types": ">= 15.7.0",
    "react": ">= 16.8.0"
  },
  "author": "Ivan Rudoy <rudooy@gmail.com>",
  "husky": {
    "hooks": {
      "pre-commit": "yarn build && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "setupFilesAfterEnv": [
      "<rootDir>jest.setup.js"
    ]
  },
  "dependencies": {}
}
